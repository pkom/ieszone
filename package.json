{
  "name": "school",
  "version": "0.0.0",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"backend/**/*.ts\" \"test/**/*.ts\"",
    "start:dev:db": "./backend/scripts/start-db.sh",
    "start:dev:db:seed": "ts-node -P tsconfig.server.json -r tsconfig-paths/register ./backend/scripts/seed.ts",
    "start:client": "ng serve",
    "start:server": "nest start --watch",
    "start": "concurrently -c \"green.bold,blue.bold\" -n \"API,SCHOOL\" \"yarn start:server\" \"yarn start:client --verbose=false\"",
    "start:prod": "node dist/api/main",
    "pretypeorm": "(rm ormconfig.json || :) && ts-node -P tsconfig.server.json -r tsconfig-paths/register ./backend/scripts/write-type-orm-config.ts",
    "typeorm": "ts-node -P tsconfig.server.json -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
    "typeorm:migration:generate": "yarn run typeorm migration:generate -n",
    "typeorm:migration:run": "yarn run typeorm migration:run",
    "typeorm:migration:revert": "yarn run typeorm migration:revert",
    "typeorm:migration:drop": "yarn run typeorm schema:drop",
    "rayuela": "node dist/api/scripts/rayuela.js",
    "rayuela:dev": "ts-node -P tsconfig.server.json -r tsconfig-paths/register backend/scripts/rayuela.ts",
    "books": "node dist/api/scripts/import-books.js",
    "books:dev": "ts-node -P tsconfig.server.json -r tsconfig-paths/register backend/scripts/import-books.ts",
    "lint": "eslint \"{backend,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "~11.0.4",
    "@angular/cdk": "^11.0.3",
    "@angular/common": "~11.0.4",
    "@angular/compiler": "~11.0.4",
    "@angular/core": "~11.0.4",
    "@angular/forms": "~11.0.4",
    "@angular/material": "^11.0.3",
    "@angular/material-moment-adapter": "^11.0.3",
    "@angular/platform-browser": "~11.0.4",
    "@angular/platform-browser-dynamic": "~11.0.4",
    "@angular/router": "~11.0.4",
    "@hapi/joi": "^17.1.1",
    "@nestjs/common": "^7.5.1",
    "@nestjs/config": "^0.6.1",
    "@nestjs/core": "^7.5.1",
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/platform-express": "^7.5.1",
    "@nestjs/swagger": "^4.7.8",
    "@nestjs/typeorm": "^7.1.5",
    "bcrypt": "^5.0.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "clear": "^0.1.0",
    "cli-color": "^2.0.0",
    "cli-progress": "^3.8.2",
    "commander": "^6.2.1",
    "compression": "^1.7.4",
    "csurf": "^1.11.0",
    "csvtojson": "^2.0.10",
    "dotenv": "^8.2.0",
    "fs": "^0.0.1-security",
    "helmet": "^4.3.1",
    "jimp": "^0.16.1",
    "lodash": "^4.17.20",
    "moment": "^2.29.1",
    "nest-winston": "^1.4.0",
    "node-geocoder": "^3.27.0",
    "passport": "^0.4.1",
    "passport-jwt": "^4.0.0",
    "passport-ldapauth": "^3.0.1",
    "pg": "^8.5.1",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "~6.6.0",
    "swagger-ui-express": "^4.1.6",
    "temp-dir": "^2.0.0",
    "tslib": "^2.0.0",
    "typeorm": "^0.2.29",
    "unzipper": "^0.10.11",
    "winston": "^3.3.3",
    "xlsx": "^0.16.9",
    "xml2js": "^0.4.23",
    "zone.js": "~0.10.2"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "~0.1100.4",
    "@angular/cli": "~11.0.4",
    "@angular/compiler-cli": "~11.0.4",
    "@nestjs/cli": "^7.5.1",
    "@nestjs/schematics": "^7.1.3",
    "@nestjs/testing": "^7.5.1",
    "@types/bcrypt": "^3.0.0",
    "@types/express": "^4.17.8",
    "@types/hapi__joi": "^17.1.6",
    "@types/jest": "^26.0.15",
    "@types/node": "^14.14.6",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.6.1",
    "@typescript-eslint/parser": "^4.6.1",
    "concurrently": "^5.3.0",
    "eslint": "^7.12.1",
    "eslint-config-prettier": "7.0.0",
    "eslint-plugin-prettier": "^3.1.4",
    "jest": "^26.6.3",
    "ng-packagr": "^11.0.0",
    "prettier": "^2.1.2",
    "supertest": "^6.0.0",
    "ts-jest": "^26.4.3",
    "ts-loader": "^8.0.8",
    "ts-node": "^9.0.0",
    "tsconfig-paths": "^3.9.0",
    "tslint": "~6.1.0",
    "typescript": "~4.0.5"
  }
}
